num1 = float(input("Ingrese el primer nÃºmero: "))
num2 = float(input("Ingrese el segundo nÃºmero: "))

print(f"\nComparaciones entre {num1} y {num2}:")
print(f"{num1} == {num2}: {num1 == num2}")
print(f"{num1} != {num2}: {num1 != num2}")
print(f"{num1} > {num2}: {num1 > num2}")
print(f"{num1} < {num2}: {num1 < num2}")
print(f"{num1} >= {num2}: {num1 >= num2}")
print(f"{num1} <= {num2}: {num1 <= num2}")
